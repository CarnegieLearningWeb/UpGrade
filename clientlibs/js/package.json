{
  "name": "upgrade_client_lib",
  "version": "1.1.7",
  "description": "Client library to communicate with the Upgrade server",
  "main": "dist/bundle.js",
  "types": "dist/clientlibs/js/src/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "npm run clean && webpack",
    "build:watch": "tsc -w",
    "clean": "rm -rf dist",
    "lint:report": "eslint -c .eslintrc.js --ext .ts './{src, test}/**/*.ts' && npm run prettier:report",
    "lint:fix": "eslint -c .eslintrc.js --ext .ts './{src, test}/**/*.ts' --fix && npm run prettier:write",
    "prettier:write": "prettier --config .prettierrc './{src, test}/**/*.ts' --write",
    "prettier:report": "prettier --config .prettierrc './{src, test}/**/*.ts' --list-different",
    "test": "jest",
    "prepare": "cd ../../ && husky install clientlibs/js/.husky"
  },
  "lint-staged": {
    "./{src, test}/**/*.ts": [
      "prettier --config .prettierrc --write",
      "eslint -c .eslintrc.js --ext .ts --fix"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "es6-promise": "^4.2.8",
    "isomorphic-fetch": "^3.0.0",
    "uuid": "^8.3.2"
  }
}
