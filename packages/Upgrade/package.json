{
    "name": "ab_testing_backend",
    "version": "1.0.1",
    "description": "Backend for A/B Testing Project",
    "main": "index.js",
    "scripts": {
        "start": "nps",
        "dev": "npm start serve",
        "build": "npm start build",
        "production": "npm start serve.production",
        "test": "npm start test",
        "test:watch": "npm start test.watch",
        "test:coverage": "npm start test.coverage",
        "seed": "npm start db.seed",
        "drop": "npm start db.drop",
        "help": "npm start help",
        "generate-data": "npm run build && node dist/src/dataGenerator/index.js",
        "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n",
        "migration:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:create -n",
        "migration:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
        "migration:revert": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert"
    },
    "keywords": [],
    "author": "",
    "jest": {
        "transform": {
            ".(ts|tsx)": "../../preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ],
        "testEnvironment": "node",
        "testTimeout": 30000,
        "setupFilesAfterEnv": [
            "./test/unit/lib/setup.ts"
        ]
    },
    "license": "ISC",
    "devDependencies": {
        "@types/body-parser": "^1.17.1",
        "@types/compression": "^1.0.1",
        "@types/cors": "^2.8.6",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.17.1",
        "@types/faker": "^4.1.8",
        "@types/helmet": "^0.0.44",
        "@types/multer": "^1.3.10",
        "@types/serve-favicon": "^2.2.31",
        "@types/swagger-jsdoc": "^3.0.2",
        "@types/swagger-ui-express": "^4.1.0",
        "copyfiles": "^2.1.1",
        "cross-env": "^6.0.3",
        "faker": "^4.1.0",
        "ncp": "^2.0.0",
        "nps": "^5.9.8",
        "nps-utils": "^1.7.0",
        "typeorm-seeding": "^1.2.0"
    },
    "dependencies": {
        "@types/jest": "^24.0.21",
        "@types/uuid": "^3.4.5",
        "aws-sdk": "^2.639.0",
        "body-parser": "^1.19.0",
        "chalk": "^2.4.2",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.10.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "csv-stringify": "^5.4.3",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "figlet": "^1.2.4",
        "google-auth-library": "^5.9.2",
        "helmet": "^3.21.2",
        "jest": "^24.9.0",
        "json-diff": "^0.5.4",
        "jsonfile": "^5.0.0",
        "jsonwebtoken": "^8.5.1",
        "lodash.flatten": "^4.4.0",
        "lodash.isequal": "^4.5.0",
        "microframework": "^0.6.4",
        "mocker-data-generator": "^2.8.0",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "node-fetch": "^2.6.0",
        "nodemon": "^1.19.4",
