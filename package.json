{
  "name": "ab_testing_backend",
  "version": "1.0.0",
  "description": "Backend for A/B Testing Project",
  "scripts": {
    "install:all": "yarn && cd packages/Schedular && yarn",
    "production:upgrade": "cd packages/Upgrade && npm run production",
    "build:upgrade": "cd packages/Upgrade && npm run build",
    "dockerConfig:development": "ts-node --transpile-only ./commands/dockerrun.ts development",
    "dockerConfig:production": "ts-node --transpile-only ./commands/dockerrun.ts production",
    "deploy:schedule": "cd packages/Schedular && sls deploy"
  },
  "license": "ISC",
  "dependencies": {
    "jsonfile": "^6.0.1"
  },
  "devDependencies": {
    "ts-node": "^8.6.2",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  }
}
