{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "strict": false,
    "alwaysStrict": false,
    "strictNullChecks": false,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "module": "es2020",
    "target": "ES2022",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "upgrade_types": [
        "../../types"
      ]
    },
    "useDefineForClassFields": false
  }
}
