{
  "name": "ab-testing",
  "version": "5.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "docker:local": "ng serve --open --host 0.0.0.0",
    "cl:build": "npm run build:prod && npm run cl:copy-package-json",
    "cl:copy-package-json": "cp projects/upgrade/package.json dist/upgrade/",
    "start:prod": "npm run build:prod && npm run server",
    "prebuild": "npm run test",
    "build": "ng build",
    "prebuild:prod": "npm run test",
    "build:prod": "ng build --configuration production",
    "build:demo": "ng build --configuration demo --base-href /upgrade/",
    "build:dev": "ng build -c=development",
    "clean": "rimraf node_modules && rm package-lock.json && npm update && npm i",
    "lint": "eslint -c ../.eslintrc.js --ext .ts './projects/upgrade/src' && npm run prettier:check",
    "lint:fix": "eslint -c ../.eslintrc.js --ext .ts './projects/upgrade/src' && npm run prettier:write",
    "test": "jest --runInBand",
    "test:coverage": "open ./coverage/lcov-report/index.html",
    "test:once": "jest --maxWorkers=50% --coverage",
    "test:watch": " jest --maxWorkers=25% --watchAll --coverage",
    "test:badges": "coverage-badge-creator",
    "test:dev": "npm-run-all --parallel test:watch test:coverage test:badges",
    "prettier:write": "prettier --config ../.prettierrc './projects/upgrade/src/**/*.{ts,js,html,json,scss}' --write",
    "prettier:check": "prettier --config ../.prettierrc './projects/upgrade/src/**/*.{ts,js,html,json,scss}' --check",
    "release": "standard-version && git push --follow-tags origin master",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/upgrade/stats.json",
    "server": "node ./projects/server/server.js",
    "server:demo": "node ./projects/server/demo-server.js",
    "build:development": "npm run build:dev",
    "build:production": "npm run build:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.8",
    "@angular/cdk": "^15.2.8",
    "@angular/common": "^15.2.8",
    "@angular/compiler": "^15.2.8",
    "@angular/core": "^15.2.8",
    "@angular/forms": "^15.2.8",
    "@angular/material": "^15.2.8",
    "@angular/platform-browser": "^15.2.8",
    "@angular/platform-browser-dynamic": "^15.2.8",
    "@angular/router": "^15.2.8",
    "@danielmoncada/angular-datetime-picker": "^16.0.1",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/entity": "^15.4.0",
    "@ngrx/router-store": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "@ngrx/store-devtools": "^15.4.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@swimlane/ngx-charts": "^20.4.1",
    "angular2-notifications": "^12.0.0",
    "ansi-to-html": "^0.6.14",
    "dayjs": "^1.11.2",
    "jszip": "^3.10.1",
    "jwt-decode": "^3.1.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.find": "^4.6.0",
    "lodash.groupby": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "ngx-skeleton-loader": "^7.0.0",
    "rxjs": "7.8.0",
    "tslib": "^2.0.0",
    "uuid": "^3.3.3",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular/cli": "^15.2.6",
    "@angular/compiler-cli": "^15.2.8",
    "@angular/language-service": "^15.2.8",
    "@types/google.accounts": "^0.0.6",
    "@types/jest": "^28.0.0",
    "@types/lodash.find": "^4.6.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^18.0.0",
    "@types/uuid": "^8.3.4",
    "codelyzer": "^6.0.2",
    "coverage-badge-creator": "^1.0.11",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.5.0",
    "jest-preset-angular": "~11.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.8",
    "ts-node": "~10.8.0",
    "typescript": "~4.9.5",
    "webpack-bundle-analyzer": "^4.6.1"
  }
}