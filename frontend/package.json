{
    "name": "ab-testing",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open",
        "docker:local": "ng serve --open --host 0.0.0.0",
        "cl:build": "npm run build:prod && npm run cl:copy-package-json",
        "cl:copy-package-json": "cp projects/abtesting/package.json dist/abtesting/",
        "start:prod": "npm run build:prod && npm run server",
        "prebuild": "npm run test",
        "build": "ng build",
        "prebuild:prod": "npm run test",
        "build:prod": "ng build --prod",
        "build:dev": "ng build -c=development",
        "lint": "ng lint",
        "test": "jest",
        "test:coverage": "open ./coverage/lcov-report/index.html",
        "test:once": "jest --coverage",
        "test:watch": " jest --watchAll --coverage",
        "test:dev": "npm-run-all --parallel test:watch test:coverage",
        "format:write": "prettier projects/**/*.{ts,json,md,scss} --write",
        "format:test": "prettier projects/**/*.{ts,json,md,scss} --list-different",
        "release": "standard-version && git push --follow-tags origin master",
        "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/abtesting/stats-es2015.json",
        "server": "node ./projects/server/server.js",
        "build:development": "npm run build:dev",
        "build:production": "npm run build:prod"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "private": true,
    "dependencies": {
        "@angular-extensions/elements": "^12.2.16",
        "@angular-extensions/model": "^12.0.0",
        "@angular/animations": "~12.2.16",
        "@angular/cdk": "~12.2.13",
        "@angular/common": "~12.2.16",
        "@angular/compiler": "~12.2.12",
        "@angular/core": "^12.2.16",
        "@angular/forms": "~12.2.16",
        "@angular/material": "~12.2.13",
        "@angular/platform-browser": "~12.2.16",
        "@angular/platform-browser-dynamic": "~12.2.16",
        "@angular/router": "~12.2.16",
        "@ngrx/effects": "^12.0.0",
        "@ngrx/entity": "^12.0.0",
        "@ngrx/router-store": "^12.0.0",
        "@ngrx/store": "^12.0.0",
        "@ngrx/store-devtools": "^12.0.0",
        "@ngx-translate/core": "^11.0.1",
        "@ngx-translate/http-loader": "^4.0.0",
        "@swimlane/ngx-charts": "^16.0.0",
        "ang-jsoneditor": "^1.9.0",
        "angular2-notifications": "^12.0.0",
        "ansi-to-html": "^0.6.14",
        "dayjs": "^1.10.8",
        "hammerjs": "^2.0.8",
        "jsoneditor": "^9.5.6",
        "lodash.clonedeep": "^4.5.0",
        "lodash.find": "^4.6.0",
        "lodash.groupby": "^4.6.0",
        "ng-pick-datetime-ex": "^12.0.1",
        "ngx-skeleton-loader": "^2.2.0",
        "npm": "^8.5.1",
        "papaparse": "^5.3.0",
        "rxjs": "~6.6.0",
        "tslib": "^1.9.0",
        "uuid": "^3.3.3",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^12.2.16",
        "@angular/cli": "^12.2.16",
        "@angular/compiler-cli": "^12.0.0",
        "@angular/language-service": "~12.2.16",
        "@types/jest": "^27.4.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^10.14.19",
        "@types/uuid": "^3.4.4",
        "codelyzer": "^6.0.2",
        "express": "^4.16.4",
        "jest": "^27.5.1",
        "jest-preset-angular": "^11.1.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.17.1",
        "pretty-quick": "^1.11.1",
        "rimraf": "^2.6.2",
        "standard-version": "^9.3.2",
        "stylelint": "^13.0.3",
        "stylelint-config-concentric-order": "^3.5.0",
        "stylelint-scss": "^3.13.0",
        "ts-jest": "^27.1.3",
        "ts-node": "~7.0.1",
        "tslint": "~6.1.0",
        "typescript": "~4.3.0",
        "webpack-bundle-analyzer": "^3.3.2"
    }
}
