{
  "name": "ab-testing",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "docker:local": "ng serve --open --host 0.0.0.0",
    "cl:build": "npm run build:prod && npm run cl:copy-package-json",
    "cl:copy-package-json": "cp projects/abtesting/package.json dist/abtesting/",
    "start:prod": "npm run build:prod && npm run server",
    "prebuild": "npm run test",
    "build": "ng build",
    "prebuild:prod": "npm run test",
    "build:prod": "ng build --configuration production",
    "build:dev": "ng build -c=development",
    "lint": "ng lint",
    "test": "jest",
    "test:coverage": "open ./coverage/lcov-report/index.html",
    "test:once": "jest --coverage",
    "test:watch": " jest --watchAll --coverage",
    "test:badges": "coverage-badge-creator",
    "test:dev": "npm-run-all --parallel test:watch test:coverage test:badges",
    "format:write": "prettier projects/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier projects/**/*.{ts,json,md,scss} --list-different",
    "release": "standard-version && git push --follow-tags origin master",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/abtesting/stats-es2015.json",
    "server": "node ./projects/server/server.js",
    "build:development": "npm run build:dev",
    "build:production": "npm run build:prod"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/cdk": "~13.3.8",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/material": "~13.3.8",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "@ngrx/effects": "~13.2.0",
    "@ngrx/entity": "~13.2.0",
    "@ngrx/router-store": "~13.2.0",
    "@ngrx/store": "~13.2.0",
    "@ngrx/store-devtools": "~13.2.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@swimlane/ngx-charts": "^20.1.0",
    "ang-jsoneditor": "^1.9.0",
    "angular2-notifications": "^12.0.0",
    "ansi-to-html": "^0.6.14",
    "dayjs": "^1.11.2",
    "jsoneditor": "^9.5.6",
    "lodash.clonedeep": "^4.5.0",
    "lodash.find": "^4.6.0",
    "lodash.groupby": "^4.6.0",
    "moment": "^2.29.4",
    "ng-pick-datetime-ex": "^13.0.1",
    "ngx-skeleton-loader": "^2.2.0",
    "rxjs": "7.4",
    "tslib": "^2.0.0",
    "uuid": "^3.3.3",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.7",
    "@angular/cli": "~13.3.7",
    "@angular/compiler-cli": "~13.3.11",
    "@angular/language-service": "~13.3.11",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.0.00",
    "@types/uuid": "^8.3.4",
    "codelyzer": "^6.0.2",
    "coverage-badge-creator": "^1.0.11",
    "jest": "^27.0.0",
    "jest-preset-angular": "~11.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.1",
    "standard-version": "^8.0.1",
    "stylelint": "^12.0.0",
    "stylelint-config-concentric-order": "^3.5.0",
    "stylelint-scss": "^3.13.0",
    "ts-jest": "^27.0.0",
    "ts-node": "~10.8.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
