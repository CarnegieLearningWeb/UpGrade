{
  "name": "ab-testing",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "docker:local": "ng serve --open --host 0.0.0.0",
    "cl:build": "npm run build:prod && npm run cl:copy-package-json",
    "cl:copy-package-json": "cp projects/abtesting/package.json dist/abtesting/",
    "start:prod": "npm run build:prod && npm run server",
    "prebuild": "npm run test",
    "build": "ng build",
    "prebuild:prod": "npm run test",
    "build:prod": "ng build --prod",
    "build:dev": "ng build -c=development",
    "lint": "ng lint",
    "test": "jest",
    "test:coverage": "open ./coverage/lcov-report/index.html",
    "test:once": "jest --coverage",
    "test:watch": " jest --watchAll --coverage",
    "test:badges": "coverage-badge-creator",
    "test:dev": "npm-run-all --parallel test:watch test:coverage test:badges",
    "format:write": "prettier projects/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier projects/**/*.{ts,json,md,scss} --list-different",
    "release": "standard-version && git push --follow-tags origin master",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/abtesting/stats-es2015.json",
    "server": "node ./projects/server/server.js",
    "build:development": "npm run build:dev",
    "build:production": "npm run build:prod"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "~11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/material": "~11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@ngrx/effects": "~11.0.0",
    "@ngrx/entity": "~11.0.0",
    "@ngrx/router-store": "~11.0.0",
    "@ngrx/store": "~11.0.0",
    "@ngrx/store-devtools": "~11.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "ang-jsoneditor": "^1.9.0",
    "ansi-to-html": "^0.6.14",
    "jsoneditor": "^9.5.6",
    "lodash.clonedeep": "^4.5.0",
    "lodash.find": "^4.6.0",
    "lodash.groupby": "^4.6.0",
    "moment": "^2.29.2",
    "ngx-skeleton-loader": "^2.2.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "uuid": "^3.3.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.19",
    "@angular/cli": "~11.2.19",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@types/jest": "^25.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.1",
    "@types/uuid": "^3.4.4",
    "codelyzer": "^5.1.2",
    "coverage-badge-creator": "^1.0.10",
    "jest": "^25.0.0",
    "jest-preset-angular": "~8.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.1",
    "standard-version": "^8.0.1",
    "stylelint": "^12.0.0",
    "stylelint-config-concentric-order": "^3.5.0",
    "stylelint-scss": "^3.13.0",
    "ts-node": "~7.0.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}