{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "upgrade": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "changeDetection": "OnPush",
          "style": "scss"
        }
      },
      "root": "projects/upgrade",
      "sourceRoot": "projects/upgrade/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/upgrade",
            "index": "projects/upgrade/src/index.html",
            "main": "projects/upgrade/src/main.ts",
            "polyfills": "projects/upgrade/src/polyfills.ts",
            "tsConfig": "projects/upgrade/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/upgrade/src/favicon.ico",
              "projects/upgrade/src/assets"
            ],
            "styles": ["projects/upgrade/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/upgrade/src/environments/environment.ts",
                  "with": "projects/upgrade/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "bundle",
                  "name": "polyfills",
                  "baseline": "150kb",
                  "maximumWarning": "50kb",
                  "maximumError": "100kb"
                },
                {
                  "type": "bundle",
                  "name": "styles",
                  "baseline": "1mb",
                  "maximumWarning": "2mb",
                  "maximumError": "100kb"
                },
                {
                  "type": "bundle",
                  "name": "main",
                  "baseline": "2mb",
                  "maximumWarning": "3mb",
                  "maximumError": "200kb"
                }
              ]
            },
            "develop": {
              "fileReplacements": [
                {
                  "replace": "projects/upgrade/src/environments/environment.ts",
                  "with": "projects/upgrade/src/environments/environment.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "bundle",
                  "name": "polyfills",
                  "baseline": "150kb",
                  "maximumWarning": "50kb",
                  "maximumError": "100kb"
                },
                {
                  "type": "bundle",
                  "name": "styles",
                  "baseline": "1mb",
                  "maximumWarning": "2mb",
                  "maximumError": "100kb"
                },
                {
                  "type": "bundle",
                  "name": "main",
                  "baseline": "2mb",
                  "maximumWarning": "3mb",
                  "maximumError": "200kb"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "projects/upgrade/src/environments/environment.ts",
                  "with": "projects/upgrade/src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "bundle",
                  "name": "polyfills",
                  "baseline": "150kb",
                  "maximumWarning": "50kb",
                  "maximumError": "100kb"
                },
                {
                  "type": "bundle",
                  "name": "styles",
                  "baseline": "1mb",
                  "maximumWarning": "2mb",
                  "maximumError": "100kb"
                },
                {
                  "type": "bundle",
                  "name": "main",
                  "baseline": "2mb",
                  "maximumWarning": "3mb",
                  "maximumError": "200kb"
                }
              ]
            },
            "bsnl": {
              "fileReplacements": [
                {
                  "replace": "projects/upgrade/src/environments/environment.ts",
                  "with": "projects/upgrade/src/environments/environment.bsnl.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "bundle",
                  "name": "polyfills",
                  "baseline": "150kb",
                  "maximumWarning": "50kb",
                  "maximumError": "100kb"
                },
                {
                  "type": "bundle",
                  "name": "styles",
                  "baseline": "1mb",
                  "maximumWarning": "2mb",
                  "maximumError": "100kb"
                },
                {
                  "type": "bundle",
                  "name": "main",
                  "baseline": "2mb",
                  "maximumWarning": "3mb",
                  "maximumError": "200kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "upgrade:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "upgrade:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "upgrade:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/upgrade/tsconfig.app.json",
              "projects/upgrade/tsconfig.spec.json",
              "projects/upgrade/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/upgrade/e2e/protractor.conf.js",
            "devServerTarget": "upgrade:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "upgrade:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "upgrade"
}
